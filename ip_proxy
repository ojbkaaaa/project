def get_ip(num):
    '''
        获取IP
        :return: 返回IP
    '''
    url = 'https://www.kuaidaili.com/free/inha/%s/' % num
    user_agent = get_user_agent()
    headers = {
        'User-Agent': user_agent
    }
    request = requests.get(url, headers=headers)
    html = BeautifulSoup(request.text, 'lxml')
    soup = html.find('table', class_='table table-bordered table-striped')
    ip_tr = soup.find('tbody').find_all('tr')
    global ip_info
    ip_proxy = {}
    for i in range(len(ip_tr)):
        info = ip_tr[i].find_all('td')
        ip = '%s:%s' % (info[0].text, info[1].text)
        ip_type = info[3].text
        ip_proxy['ip'] = ip
        ip_proxy['ip_type'] = ip_type
        ip_info.append(ip_proxy)
        ip_proxy = {}
        # print(ip_info)
    # ip = thead[0]
    # ip_type = thead[3]

def ip_true(i):
    '''
    验证ip
    :return: 可用ip
    '''
    global http_ip, https_ip
    urls = "https://www.baidu.com/"
    url = ''
    # ip = ip_info.pop()
    addr = i['ip']
    status = i['ip_type']
    if status == 'HTTP':
        proxies = {
            'http': addr
        }
        try:
            request = requests.get(urls, proxies=proxies, timeout=60)
            if request.status_code == 200:
                http_ip.append(addr)
        except requests.RequestException as e:
            print("验证代理IP:" + addr + "时发生如下错误 :")
            print(e)
    if status == 'HTTPS':
        proxies = {
            'https': addr
        }
        try:
            request = requests.get(urls, proxies=proxies, timeout=60)
            if request.status_code == 200:
                https_ip.append(addr)
        except requests.RequestException as e:
            print("验证代理IP" + addr + "时发生如下错误 :")
            print(e)

if __name__ == '__main__':
    for i in range(1, 5):
        time.sleep(1)
        get_ip(i)
    # print(ip_info)
    for i in range(len(ip_info)):
        i = ip_info[i]
        print(i)
        # ip_true(i)
    print(len(http_ip))
    print(len(https_ip))
    # print(len(ip_info))
